    <!DOCTYPE html>
    <html>
      <head>
        <style>
           #map {
            height: 400px;
            width: 400px;
           }
        </style>
      </head>
      <body bgcolor= "#D7CCC8">
        <h1>Ubicaci√≥n</h1>
        <div id="map"></div>
        <script>
          function initMap() {
            var uluru = {lat:21.0139808, lng: -89.578461};
            var map = new google.maps.Map(document.getElementById('map'), {
              zoom: 15,
              center: uluru
            });
            var marker = new google.maps.Marker({
              position: uluru,
              animation: google.maps.Animation.DROP,
              map: map,
              title:"Hello World!"

            });
          }
        </script>
        <script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBa4ZV4WoF3zyuJhsEbGCkX3-HJjQdF2-A&callback=initMap">
        </script>
      </body>
    </html>
